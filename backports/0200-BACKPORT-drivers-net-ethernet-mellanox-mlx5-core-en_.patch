 	struct mlx5e_encap_entry *e; /* attached encap instance */
 	struct list_head list;
+#endif
 	int index;
 };
 
@@ -118,7 +127,9 @@ struct mlx5e_tc_flow {
 	struct encap_flow_item encaps[MLX5_MAX_FLOW_FWD_VPORTS];
 	struct mlx5e_tc_flow    *peer_flow;
